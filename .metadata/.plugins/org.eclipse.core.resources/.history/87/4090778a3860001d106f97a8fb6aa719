package com.nit.assisnment;

public class AssociationTest {
	public static void main(String[] args) {
		Contractor contract=new Contractor();
		System.out.print("Contract Name:\t\t");
		contract.getContractName();
		System.out.print("Contractor Phone Number:\t\t");
		contract.getPhoneNumber();
		System.out.print("Contractor Address:\t\t");
		contract.getAddress();
		System.out.print("Contract Owner Name:\t\t");
		contract.getOwnerName();
		contract.getBuilding();
	}
}
